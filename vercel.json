{
  "version": 2,
  "buildCommand": "npm run build:web",
  "outputDirectory": "apps/web/.next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "apps/web/app/**/*.tsx": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://api-staging.agentic-social.com/api/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://api-staging.agentic-social.com",
    "NEXT_PUBLIC_WS_URL": "wss://api-staging.agentic-social.com"
  },
  "regions": ["iad1", "sfo1"]
}