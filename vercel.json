{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://api-staging.agentic-social.com",
    "NEXT_PUBLIC_WS_URL": "wss://api-staging.agentic-social.com"
  }
}